{
  "name": "knub-command-manager",
  "version": "6.1.0",
  "description": "An easy-to-use library for defining and matching text-based commands with arguments and options",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "test": "mocha --exit -r ts-node/register src/**/*.test.ts -- --trace-warnings",
    "build": "touch dist && rm -r dist && tsc",
    "preversion": "npm run test",
    "prepublishOnly": "npm run test && npm run build",
    "watch": "touch dist && rm -r dist && tsc --watch",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [],
  "author": "Miikka Virtanen <contact@mivir.fi>",
  "license": "MIT",
  "dependencies": {
    "escape-string-regexp": "^2.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.2",
    "@types/escape-string-regexp": "^2.0.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.5",
    "chai": "^4.2.0",
    "husky": "^3.0.5",
    "lint-staged": "^9.2.5",
    "mocha": "^6.2.0",
    "prettier": "^1.18.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "files": [
    "/dist"
  ]
}
